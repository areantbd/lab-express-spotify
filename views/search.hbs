<div class="artists">
  {{#each artists as |artist|}}
    <div class="card" style="width: 18rem;">
      <img src="{{artist.images.0.url}}" class="card-img-top" alt="no image" />
      <div class="card-body">
        <h5 class="card-title">{{artist.name}}</h5>
        <a href="/albums/{{artist.id}}">View Albums</a>
        <a class="spotify" href="{{artist.uri}}">View on Spotify</a>
      </div>
    </div>
  {{/each}}
</div>